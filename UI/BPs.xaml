<Window x:Class="UI.BPs"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Бизнес процессы" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dataGrid" AutoGenerateColumns="True" Margin="0,29,453,0" Width="300" HorizontalAlignment="Left"
                  SelectionChanged="dataGrid_SelectionChanged"/>
        <ToolBar Height="25" VerticalAlignment="Top">
            <Button x:Name="back" Content="Назад" Click="back_Click" />
            <Separator />
            <Button x:Name="forward" Content="Дальше" Click="forward_Click"/>
            <Separator />
            <Button x:Name="openCases" Content="Открыть кейсы" Click="openCases_Click"/>
        </ToolBar>


    </Grid>
</Window>
